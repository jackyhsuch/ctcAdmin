<table class = "table table-hover">
    <caption>Towers List</caption>
   
    <thead>
        <tr>
            <th>Name</th>
            <th>Created at</th>
        </tr>
    </thead>

    <tbody>
        <% @towers.each do |tower| %>
        <tr>
            <td><%= tower.name %></td>
            <td><%= tower.created_at %></td>
        </tr>
        <% end %>
    </tbody>
   
</table>
